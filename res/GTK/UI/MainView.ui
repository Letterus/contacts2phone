<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="granite" version="7.0"/>
  <object class="GtkWindow" id="mainWindow">
    <property name="default-height">480</property>
    <property name="default-width">640</property>
    <property name="title" translatable="yes">Transfer contacts to SNOM IP phone base</property>
    <property name="child">
      <object class="GtkPaned" id="hpaned">
        <property name="orientation">horizontal</property>
        <property name="width-request">640</property>
        <property name="height-request">360</property>
        <child>
          <object class="AdwToolbarView" id="toolbarList">
            <property name="bottom-bar-style">ADW_TOOLBAR_RAISED</property>
            <property name="width-request">320</property>
            <child type="top">
              <object class="AdwHeaderBar"/>
            </child>
            <property name="content">
              <object class="GtkScrolledWindow" id="scrolledAddressbookWindow">

              </object>
            </property>
            <child type="bottom">
              <object class="GtkActionBar">
                <child type="end">
                  <object class="GtkButton" id="refreshButton">
                    <property name="label">Refresh</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwToolbarView" id="toolbarForm">
            <property name="width-request">320</property>
            <property name="content">
              <object class="GtkScrolledWindow" id="scrolledFormWindow">
                <property name="child">
                  <object class="GtkBox" id="credentialsBox">
                    <property name="orientation">vertical</property>
                    <property name="margin-top">12</property>
                    <property name="margin-bottom">12</property>
                    <property name="margin-start">12</property>
                    <property name="margin-end">12</property>

                   </object>
                </property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>